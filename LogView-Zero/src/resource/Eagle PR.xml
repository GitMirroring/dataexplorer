<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V50.xsd">
    <Device>
    	<name implementation="gde.device.logview.ZeroAdapter">Eagle PR</name>
        <manufacturer>RC-Electronics</manufacturer>
        <manufacturerURL>https://www.rc-electronics.eu/</manufacturerURL>
        <group>logger</group>
        <image>Eagle.png</image>
        <usage>true</usage>
    </Device>
	<TimeBase>
		<name>time</name>
		<symbol>t</symbol>
		<unit>msec</unit>
		<timeStep>-1.0</timeStep>
	</TimeBase>
	<DataBlock>
		<format inputType="FILE_IO" formatType="VALUE" size="-100" />
		<timeUnit>msec</timeUnit>
		<separator>semicolon</separator>
		<leader></leader>
		<preferredFileExtention>*.txt</preferredFileExtention>
	</DataBlock>
	<State>
		<property type="Integer" value="01"
			name="state_flight_recording" />
	</State>
	<Channels>
		<Channel name="type_config_1" type="TYPE_CONFIG">
			<Measurement>
				<name>0????</name>
				<symbol>-</symbol>
				<unit></unit>
				<active>true</active>
			</Measurement>
            <Settlements/>
            <ReferenceGroups/>
            <TransitionGroups/>
            <Transitions/>
            <ScoreGroups/>
        </Channel>
    </Channels>
    <Desktop>
        <property name="table_tab" value="true"/>
        <property name="digital_tab" value="true"/>
        <property name="analog_tab" value="false"/>
        <property name="voltage_per_cell_tab" value="false"/>
        <property name="utility_garphics_tab" value="false" description="UTILITY_GRAPHICS_TAB"/>
        <property name="utility_device_tab" value="false" description="UTILITY_DEVICE_TAB"/>
    </Desktop>
</DeviceProperties>
